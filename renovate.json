{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": ["config:recommended"],
	"packageRules": [
		{
			"matchPackageNames": [
				"@expo*/*",
				"@react-native*/*",
				"@react-navigation*/*",
				"expo*",
				"react",
				"react-dom",
				"react-native",
				"react-native-gesture-handler",
				"react-native-reanimated",
				"react-native-safe-area-context",
				"react-native-screens",
				"react-native-svg",
				"react-native-web",
				"@babel*/*",
				"@types/jest",
				"@types/react",
				"jest",
				"jest-expo",
				"typescript",
				"@sinclair/typebox"
			],
			"enabled": false
		}
	]
}
